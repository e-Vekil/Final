<!-- Start Main Background Video -->
<div class="page-cover">
    <div id="container" class="video-container d-none d-sm-block">
        <video id="mainVideo" autoplay="autoplay" loop="loop" muted="muted">
            <source src="~/videos/original.webm" type="video/mp4">
        </video>
    </div>
</div>
<!-- Main Background Video End -->

<!-- SECTIONS -->
<!-- INTRODUCTION PART -->
<section class="intro sect" id="main2">
    <div class="container">
        <div class="row">
            <!-- title -->
            <header class="col-lg-7 intro-title">
                <h1>İstənİlən Hüquqİ Sənəd Hər Yerdə,Hər Zaman</h1>
            </header>
            <!-- description -->
            <div class="col-lg-7 intro-desc">
                <h5>
                    Tələbinizə uyğun sənədi hazırlayın və ya yüzlərlə müxtəlif növ sənəd nümunələri arasından lazım olanını
                    kompüterinizə yükləyin.
                </h5>
            </div>
            <!-- isotop -->
            <div class="isotop">
                <vc:category></vc:category>
            </div>
        </div>
    </div>

    <!-- Slider List -->
    <vc:slider></vc:slider>
    <!-- Slider List End -->
</section>
<!-- INTRODUCTION END -->

<div class="container">
    @* Categories *@
    <ul class="categories">
