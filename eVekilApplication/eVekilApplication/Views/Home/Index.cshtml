<section id="ContactUs" class="sect contactUs">
    <div class="container">
        <div class="row">
            <header class="contact-header col-lg-12">
                <h1>BİZİMLƏ <span>ƏLAQƏ</span></h1>
            </header>
            <div class="contact-content col-lg-12">
                <form action="/" method="post">
                    <div class="row">
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="Name">Ad</label>
                                <input type="text" class="form-control" id="Name" required>
                            </div>
                            <div class="form-group">
                                <label for="Soyad">Soyad</label>
                                <input type="text" class="form-control" id="Name" required>
                            </div>

                        </div>
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label for="Phone">Telefon</label>
                                <input type="tel" pattern="[0-9]{3}[0-9]{3}[0-9]{2}[0-9]{2}" class="form-control" id="Phone" required>
                            </div>
                            <div class="form-group">
                                <label for="Email">Email</label>
                                <input type="email" class="form-control" id="Email" required>
                            </div>
                        </div>
                        <div class="col-12">
                            <div class="form-group">
                                <label for="Question">Sualınız</label>
                                <textarea class="form-control" id="Question" rows="4" required></textarea>
                            </div>
                        </div>
                        <div class="col-12">
                            <div class="form-group">
                                <button type="submit" style="background:#8a1919; color:white;" class="btn btn-danger">Göndər</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>

