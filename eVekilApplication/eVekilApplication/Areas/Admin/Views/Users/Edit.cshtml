@model EditUserViewModel
           <!-- Row -->
<div class="row">
    <div class="col-lg-8 col-xlg-9 col-md-7" style="margin:auto;">
        <div class="card">
            <div class="card-block">
                <form asp-action="Edit" asp-controller="Users" method="post" class="form-horizontal form-material">
                    <div class="alert-danger" asp-validation-summary="ModelOnly"></div>
                    <div class="form-group">
                        <label asp-for="Password" class="col-md-12"></label>
                        <div class="col-md-12">
                            <input asp-for="Password" class="form-control form-control-line">
                            <span asp-validation-for="Password"></span>
                        </div>
                    </div>
                    <div class="form-group">
                        <label asp-for="ConfirmPassword" class="col-md-12"></label>
                        <div class="col-md-12">
                            <input asp-for="ConfirmPassword" class="form-control form-control-line">
                            <span asp-validation-for="ConfirmPassword"></span>
                        </div>
                    </div>
                    <input asp-for="Id" type="hidden" value="@Model.Id" />
                    <input type="hidden" name="changepassword" value="true" />

                    <input type="submit" value="Send" />
                </form>
                <form asp-action="Edit" asp-controller="Users" method="post" class="form-horizontal form-material">
                    <div class="form-group">
                        <label class="col-md-12">Advocates</label>
                        <div class="col-md-12">
                            <select name="roles" class="form-control form-control-line">
                                <option value="User">User</option>
                                <option value="Admin">Admin</option>
                            </select>
                        </div>
                    </div>
                    <input type="hidden" name="changepassword" value="false" />
                    <input type="submit" value="Send" />
                </form>
            </div>
        </div>
    </div>
    <!-- Column -->
</div>
<!-- Row -->




<script src="~/js/jquery.min.js"></script>


