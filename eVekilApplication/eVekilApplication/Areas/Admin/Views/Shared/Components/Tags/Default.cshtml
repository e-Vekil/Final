@model List<Tags>

@if (@ViewBag.Edit == "true")
{

    <div class="form-group">
        <label class="col-md-12">Document Tags</label>
        <div class="col-md-12">
            <ul>
                @foreach (Tags item in Model)
                {
                    if (item.DocumentId == ViewBag.DocId)
                    {
                        <li>@item.Tagname</li>
                    }

                }
            </ul>
        </div>
    </div>
}
<div class="form-group">
    <label class="col-md-12">Select Tags</label>
    <div class="col-lg-12">
        <select class="js-example-basic-multiple" name="tags" multiple="multiple">
            @foreach (Tags tag in Model)
            {
               <option value="@tag.Tagname">@tag.Tagname</option>
            }
        </select>
    </div>
</div>